generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  relationMode = "prisma"
}

model User {
  id string
}

enum Role{
  USER
  ADMIN
  SELLER
  
}

model Post {
  id        String  @id @default(uuid())
  name     String
  email   String @unique
  picture    String @db.Text
  role Role @default(USER)
  
  }